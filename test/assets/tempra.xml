	<directions>
		<direction>
			<label lang="en">Do not take more than directed (see liver warnings). Adults and children 12 years and over:</label>
			<label lang="fr"></label>
		</direction>
		<direction>
			<label lang="en">if pain or fever does not respond to 1 (tablet/caplet/etc.), take 2 (tablets/caplets/etc.) at next dose</label>
			<label lang="fr"></label>
		</direction>
		<direction>
			<label lang="en">take 1 dose every 4 to 6 hours</label>
			<label lang="fr"></label>
		</direction>
		<direction>
			<label lang="en">do not take more than 600 mg (5 doses) in 24 hours</label>
			<label lang="fr"></label>
		</direction>
		
