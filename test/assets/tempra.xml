			<type>
				<label lang="en">Ask a doctor</label>
				<label lang="fr"></label>
			</type>
			<item>
				<label lang="en">before use if you have liver or kidney disease</label>
				<label lang="fr"></label>
			</item>
			<item>
				<label lang="en">or pharmacist before use if you take the blood thinning drug warfarin</label>
				<label lang="fr"></label>
			</item>
		</warning>
		<warning>
			<type>
				<label lang="en">Stop use and ask a doctor</label>
